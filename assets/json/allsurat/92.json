    "ayat": [
      {
        "nomorAyat": 1,
        "teksArab": "وَالَّيْلِ اِذَا يَغْشٰىۙ",
        "teksIndonesia": "Demi malam apabila menutupi (cahaya siang),",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092001.mp3"
        }
      },
      {
        "nomorAyat": 2,
        "teksArab": "وَالنَّهَارِ اِذَا تَجَلّٰىۙ",
        "teksIndonesia": "demi siang apabila terang benderang,",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092002.mp3"
        }
      },
      {
        "nomorAyat": 3,
        "teksArab": "وَمَا خَلَقَ الذَّكَرَ وَالْاُنْثٰىٓ ۙ",
        "teksIndonesia": "demi penciptaan laki-laki dan perempuan,",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092003.mp3"
        }
      },
      {
        "nomorAyat": 4,
        "teksArab": "اِنَّ سَعْيَكُمْ لَشَتّٰىۗ",
        "teksIndonesia": "sungguh, usahamu memang beraneka macam.",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092004.mp3"
        }
      },
      {
        "nomorAyat": 5,
        "teksArab": "فَاَمَّا مَنْ اَعْطٰى وَاتَّقٰىۙ",
        "teksIndonesia": "Maka barangsiapa memberikan (hartanya di jalan Allah) dan bertakwa,",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092005.mp3"
        }
      },
      {
        "nomorAyat": 6,
        "teksArab": "وَصَدَّقَ بِالْحُسْنٰىۙ",
        "teksIndonesia": "dan membenarkan (adanya pahala) yang terbaik (surga),",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092006.mp3"
        }
      },
      {
        "nomorAyat": 7,
        "teksArab": "فَسَنُيَسِّرُهٗ لِلْيُسْرٰىۗ",
        "teksIndonesia": "maka akan Kami mudahkan baginya jalan menuju kemudahan (kebahagiaan),",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092007.mp3"
        }
      },
      {
        "nomorAyat": 8,
        "teksArab": "وَاَمَّا مَنْۢ بَخِلَ وَاسْتَغْنٰىۙ",
        "teksIndonesia": "dan adapun orang yang kikir dan merasa dirinya cukup (tidak perlu pertolongan Allah),",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092008.mp3"
        }
      },
      {
        "nomorAyat": 9,
        "teksArab": "وَكَذَّبَ بِالْحُسْنٰىۙ",
        "teksIndonesia": "serta mendustakan (pahala) yang terbaik,",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092009.mp3"
        }
      },
      {
        "nomorAyat": 10,
        "teksArab": "فَسَنُيَسِّرُهٗ لِلْعُسْرٰىۗ",
        "teksIndonesia": "maka akan Kami mudahkan baginya jalan menuju kesukaran (kesengsaraan),",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092010.mp3"
        }
      },
      {
        "nomorAyat": 11,
        "teksArab": "وَمَا يُغْنِيْ عَنْهُ مَالُهٗٓ اِذَا تَرَدّٰىٓۙ",
        "teksIndonesia": "dan hartanya tidak bermanfaat baginya apabila dia telah binasa.",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092011.mp3"
        }
      },
      {
        "nomorAyat": 12,
        "teksArab": "اِنَّ عَلَيْنَا لَلْهُدٰىۖ",
        "teksIndonesia": "Sesungguhnya Kamilah yang memberi petunjuk,",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092012.mp3"
        }
      },
      {
        "nomorAyat": 13,
        "teksArab": "وَاِنَّ لَنَا لَلْاٰخِرَةَ وَالْاُوْلٰىۗ",
        "teksIndonesia": "dan sesungguhnya milik Kamilah akhirat dan dunia itu.",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092013.mp3"
        }
      },
      {
        "nomorAyat": 14,
        "teksArab": "فَاَنْذَرْتُكُمْ نَارًا تَلَظّٰىۚ",
        "teksIndonesia": "Maka Aku memperingatkan kamu dengan neraka yang menyala-nyala,",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092014.mp3"
        }
      },
      {
        "nomorAyat": 15,
        "teksArab": "لَا يَصْلٰىهَآ اِلَّا الْاَشْقَىۙ",
        "teksIndonesia": "yang hanya dimasuki oleh orang yang paling celaka,",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092015.mp3"
        }
      },
      {
        "nomorAyat": 16,
        "teksArab": "الَّذِيْ كَذَّبَ وَتَوَلّٰىۗ",
        "teksIndonesia": "yang mendustakan (kebenaran) dan berpaling (dari iman).",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092016.mp3"
        }
      },
      {
        "nomorAyat": 17,
        "teksArab": "وَسَيُجَنَّبُهَا الْاَتْقَىۙ",
        "teksIndonesia": "Dan akan dijauhkan darinya (neraka) orang yang paling bertakwa, ",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092017.mp3"
        }
      },
      {
        "nomorAyat": 18,
        "teksArab": "الَّذِيْ يُؤْتِيْ مَالَهٗ يَتَزَكّٰىۚ",
        "teksIndonesia": "yang menginfakkan hartanya (di jalan Allah) untuk membersihkan (dirinya),",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092018.mp3"
        }
      },
      {
        "nomorAyat": 19,
        "teksArab": "وَمَا لِاَحَدٍ عِنْدَهٗ مِنْ نِّعْمَةٍ تُجْزٰىٓۙ",
        "teksIndonesia": "dan tidak ada seorang pun memberikan suatu nikmat padanya yang harus dibalasnya,",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092019.mp3"
        }
      },
      {
        "nomorAyat": 20,
        "teksArab": "اِلَّا ابْتِغَاۤءَ وَجْهِ رَبِّهِ الْاَعْلٰىۚ",
        "teksIndonesia": "tetapi (dia memberikan itu semata-mata) karena mencari keridaan Tuhannya Yang Mahatinggi.",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092020.mp3"
        }
      },
      {
        "nomorAyat": 21,
        "teksArab": "وَلَسَوْفَ يَرْضٰى ࣖ",
        "teksIndonesia": "Dan niscaya kelak dia akan mendapat kesenangan (yang sempurna).",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/092021.mp3"
        }
      }
