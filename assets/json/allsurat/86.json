    "ayat": [
      {
        "nomorAyat": 1,
        "teksArab": "وَالسَّمَاۤءِ وَالطَّارِقِۙ ",
        "teksIndonesia": "Demi langit dan yang datang pada malam hari.",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/086001.mp3"
        }
      },
      {
        "nomorAyat": 2,
        "teksArab": "وَمَآ اَدْرٰىكَ مَا الطَّارِقُۙ ",
        "teksIndonesia": "Dan tahukah kamu apakah yang datang pada malam hari itu?",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/086002.mp3"
        }
      },
      {
        "nomorAyat": 3,
        "teksArab": "النَّجْمُ الثَّاقِبُۙ ",
        "teksIndonesia": "(yaitu) bintang yang bersinar tajam,",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/086003.mp3"
        }
      },
      {
        "nomorAyat": 4,
        "teksArab": "اِنْ كُلُّ نَفْسٍ لَّمَّا عَلَيْهَا حَافِظٌۗ ",
        "teksIndonesia": "setiap orang pasti ada penjaganya.",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/086004.mp3"
        }
      },
      {
        "nomorAyat": 5,
        "teksArab": "فَلْيَنْظُرِ الْاِنْسَانُ مِمَّ خُلِقَ ",
        "teksIndonesia": "Maka hendaklah manusia memperhatikan dari apa dia diciptakan.",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/086005.mp3"
        }
      },
      {
        "nomorAyat": 6,
        "teksArab": "خُلِقَ مِنْ مَّاۤءٍ دَافِقٍۙ ",
        "teksIndonesia": "Dia diciptakan dari air (mani) yang terpancar,",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/086006.mp3"
        }
      },
      {
        "nomorAyat": 7,
        "teksArab": "يَّخْرُجُ مِنْۢ بَيْنِ الصُّلْبِ وَالتَّرَاۤىِٕبِۗ ",
        "teksIndonesia": "yang keluar dari antara tulang punggung (sulbi) dan tulang dada.",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/086007.mp3"
        }
      },
      {
        "nomorAyat": 8,
        "teksArab": "اِنَّهٗ عَلٰى رَجْعِهٖ لَقَادِرٌۗ ",
        "teksIndonesia": "Sungguh, Allah benar-benar kuasa untuk mengembalikannya (hidup setelah mati).",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/086008.mp3"
        }
      },
      {
        "nomorAyat": 9,
        "teksArab": "يَوْمَ تُبْلَى السَّرَاۤىِٕرُۙ ",
        "teksIndonesia": "Pada hari ditampakkan segala rahasia,",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/086009.mp3"
        }
      },
      {
        "nomorAyat": 10,
        "teksArab": "فَمَا لَهٗ مِنْ قُوَّةٍ وَّلَا نَاصِرٍۗ ",
        "teksIndonesia": "maka manusia tidak lagi mempunyai suatu kekuatan dan tidak (pula) ada penolong.",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/086010.mp3"
        }
      },
      {
        "nomorAyat": 11,
        "teksArab": "وَالسَّمَاۤءِ ذَاتِ الرَّجْعِۙ ",
        "teksIndonesia": "Demi langit yang mengandung hujan,",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/086011.mp3"
        }
      },
      {
        "nomorAyat": 12,
        "teksArab": "وَالْاَرْضِ ذَاتِ الصَّدْعِۙ ",
        "teksIndonesia": "dan bumi yang mempunyai tumbuh-tumbuhan,",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/086012.mp3"
        }
      },
      {
        "nomorAyat": 13,
        "teksArab": "اِنَّهٗ لَقَوْلٌ فَصْلٌۙ ",
        "teksIndonesia": "sungguh, (Al-Qur'an) itu benar-benar firman pemisah (antara yang hak dan yang batil),",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/086013.mp3"
        }
      },
      {
        "nomorAyat": 14,
        "teksArab": "وَّمَا هُوَ بِالْهَزْلِۗ ",
        "teksIndonesia": "dan (Al-Qur'an) itu bukanlah sendagurauan.",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/086014.mp3"
        }
      },
      {
        "nomorAyat": 15,
        "teksArab": "اِنَّهُمْ يَكِيْدُوْنَ كَيْدًاۙ ",
        "teksIndonesia": "Sungguh, mereka (orang kafir) merencanakan tipu daya yang jahat.",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/086015.mp3"
        }
      },
      {
        "nomorAyat": 16,
        "teksArab": "وَّاَكِيْدُ كَيْدًاۖ ",
        "teksIndonesia": "Dan Aku pun membuat rencana (tipu daya) yang jitu.",
        "audio": {
          "05": "https://equran.nos.wjv-1.neo.id/audio-partial/Misyari-Rasyid-Al-Afasi/086016.mp3"
        }
      },
      {
        "nomorAyat": 17,
        "teksArab": "فَمَهِّلِ الْكٰفِرِيْنَ اَمْهِلْهُمْ رُوَيْدًا ࣖ ",
        "teksIndonesia": "Karena itu berilah penangguhan kepada orang-orang kafir itu. Berilah mereka itu kesempatan untuk sementara waktu.",
